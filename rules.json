[
    {
        "from": "--",
        "to": "—"
    },
    {
        "from": "[^=]\"([^=]*?)\"",
        "to": "«&nbsp;$1&nbsp;»"
    },    
    {
        "from": "'",
        "to": "’"
    },
    {
        "from": " :",
        "to": "&nbsp;:"
    },
    {
        "from": " ([;|?|!])",
        "to": "&#8239;$1"
    },
    {
        "from": "[ ]{2,}",
        "to": " "
    },    
    {
        "from": "(?!^)\\((\\d*)\\)",
        "to": "<sup><a href=\"#footnote-$1\" id=\"fn-ref-$1\">$1</a></sup>"
    },
    {
        "from": "^\\((\\d*)\\)",
        "to": "<sup><a href=\"#fn-ref-$1\" id=\"footnote-$1\">$1</a></sup>"
    }    
]
